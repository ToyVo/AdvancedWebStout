{
  "name": "advancedwebstout",
  "version": "1.0.0",
  "description": "Anything that I am doing during my Advanced Web and Internet Programming class",
  "main": "src/server/server.js",
  "private": true,
  "scripts": {
    "start:dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server": "babel-node src/server/server.js",
    "server:dev": "DEBUG=server nodemon src/server/server.js",
    "server:dev:win": "set DEBUG=app & nodemon src/server/server.js",
    "client": "webpack --mode production",
    "client:dev": "webpack --mode development --watch true",
    "client:dev-server": "webpack-dev-server --mode development --watch true",
    "lint": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ToyVo/AdvancedWebStout.git"
  },
  "author": "Collin Diekvoss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ToyVo/AdvancedWebStout/issues"
  },
  "homepage": "https://github.com/ToyVo/AdvancedWebStout#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "bootstrap": "^4.4.1",
    "chalk": "^3.0.0",
    "css-loader": "^3.5.2",
    "debug": "^4.1.1",
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.2.0",
    "jquery": "^3.5.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.3",
    "popper.js": "^1.16.1",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.1.4",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jquery": "^3.3.34",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "babel-node"
    },
    "watch": [
      "src/server/**/*"
    ],
    "restartabel": "rs",
    "ignore": [
      "node_modules/**/node_modules",
      "public/**"
    ],
    "delay": 2500,
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  }
}
